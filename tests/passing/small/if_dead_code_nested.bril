# ARGS: 1
@main(arg: int) {
  zero: int = const 0;
  isless: bool = le arg zero;
  br isless .less .greater;
.less:
  result: int = const 1;
  result_predicate: bool = const true;

  inner_pred: bool = lt arg zero;
  br inner_pred .inner_a .inner_b;
.inner_a:
  x: int = const 1;
  jmp .inner_done;
.inner_b:
  x: int = const 2;
.inner_done:

  jmp .done;
.greater:
  result: int = const 0;
  result_predicate: bool = const false;
.done:
  print result;
  print result_predicate;
  print x;
}
