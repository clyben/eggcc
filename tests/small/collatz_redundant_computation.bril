# ARGS: 8888
@main (arg : int) {
	three : int = const 3;
	two : int = const 2;
	one : int = const 1;
	zero : int = const 0;
	var : int = id arg;
.cond:
	cond : bool = eq var one;
	br cond .break .body;
.body:
	d : int = div var two;
	m : int = mul d two;
	mod : int = sub var m;
	cond : bool = eq mod zero;
	var_then : int = div var two;
    v1_else : int = mul three var;
	var_else : int = add one v1_else;
	br cond .then .else;
.then:
    var : int = id var_then;
	jmp .cond;
.else:
    var : int = id var_else;
	jmp .cond;
.break:
	print arg;
}
