@main() {
  x: int = const 1;
  y: int = const 2;
  z: int = const 3;
  a: bool = lt x y;
  b: bool = lt y z;

  c: bool = and a b;
  br c .A .B;
.A:
  print x;
  jmp .C;
.B:
  print y;
.C:
  print z;
}

