@main() {
  i: int = const 0;

.top:
  a: int = const 5;
  if_cond: bool = lt i a;
  br if_cond .then_if .else_if;
.then_if:
  one: int = const 1;
  print one;
  jmp .end_if;
.else_if:
  two: int = const 2;
  print two;
.end_if:

  one: int = const 1;
  i: int = add i one;

  b: int = const 5;
  loop_cond: bool = lt i b;
  br loop_cond .top .end;
.end:

  print i;
}
