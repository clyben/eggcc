# ARGS: 2 4
@main(a: int, b: int) {
  one: int = const 1;
  two: int = const 2;
  three: int = const 3;
  four: int = const 4;


  # a + 3
  a2: int = add a three;
  # b + 4
  b4: int = add b four;
  # (a + 3) + (b + 4)
  a2b4: int = add a2 b4;

  # 1 + a2b4
  onea2b4: int = add one a2b4;

  print onea2b4;
}